{
  "description": "Filter by fees older than certain number of days",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "default": "Age"
    },
    "numDays": {
      "type": "integer",
      "minimum": 1
    },
    "condition": {
      "type": "string",
      "enum": [
        "LESS_THAN_EQUAL",
        "LESS_THAN",
        "GREATER_THAN",
        "GREATER_THAN_EQUAL"
      ]
    }

  },
  "additionalProperties": false,
  "required": ["type", "numDays", "condition"]
}
