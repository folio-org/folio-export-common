{
  "description": "Token to represent part of date",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "default": "Date"
    },
    "value": {
      "type": "string",
      "enum": [
        "YEAR_LONG",
        "YEAR_SHORT",
        "MONTH",
        "DATE",
        "HOUR",
        "MINUTE",
        "SECOND",
        "QUARTER",
        "WEEK_OF_YEAR",
        "WEEK_OF_YEAR_ISO",
        "WEEK_YEAR",
        "WEEK_YEAR_ISO"
      ]
    },
    "timezone": {
      "type": "string"
    },
    "lengthControl": {
      "$ref": "bursarExportTokenLengthControl.json"
    }
  },
  "additionalProperties": false,
  "required": ["type", "value", "timezone"]
}
