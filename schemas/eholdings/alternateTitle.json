{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Alternate Title Schema",
  "description": "Alternate Title Schema",
  "AlternateTitle": {
    "type": "object",
    "properties": {
      "alternateTitle": {
        "type": "string",
        "description": "alternate title name",
        "example": "Boston Coll Law Rev"
      },
      "titleType": {
        "type": "string",
        "description": "alternate title type",
        "example": "Abbreviated"
      }
    }
  }
}
