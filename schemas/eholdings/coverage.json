{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Coverage Schema",
  "description": "Coverage Schema",
  "Coverage": {
    "type": "object",
    "properties": {
      "beginCoverage": {
        "type": "string",
        "description": "Begin Coverage Date",
        "example": "2003-01-01"
      },
      "endCoverage": {
        "type": "string",
        "description": "End Coverage Date",
        "example": "2003-12-01"
      }
    }
  }
}
