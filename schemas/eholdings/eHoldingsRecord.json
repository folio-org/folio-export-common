  "description": "The schema describes an export record",
  "EHoldingsRecord": {
    "type": "object",
    "properties": {
      "package": {
        "type": "object",
        "description": "Package to export",
        "$ref": "eHoldingsPackage.json#/EHoldingsPackage"
      },
      "titles": {
        "type": "array",
        "description": "Titles related exported package",
        "items": {
