{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Proxy Schema",
  "description": "Proxy Schema",
  "Proxy": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Proxy Id",
        "example": "EZProxy"
      },
      "inherited": {
        "type": "boolean",
        "description": "Whether this proxy is inherited",
        "example": true
      }
    }
  }
}
