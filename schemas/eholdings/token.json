{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Token Schema",
  "description": "Token Schema",
  "Token": {
    "type": "object",
    "properties": {
      "factName": {
        "type": "string",
        "description": "Fact Name",
        "example": "[[galesiteid]]"
      },
      "prompt": {
        "type": "string",
        "description": "Prompt",
        "example": "/itweb/"
      },
      "helpText": {
        "type": "string",
        "description": "Help text",
        "example": "What is token and how to use it"
      },
      "value": {
        "type": "string",
        "description": "Value",
        "example": "hellotoken"
      }
    }
  }
}
